{
  "arrowParens": "avoid",
  "bracketSpacing": true,
  "insertPragma": false,
  "printWidth": 100,
  "proseWrap": "always",
  "quoteProps": "as-needed",
  "requirePragma": false,
  "semi": true,
  "singleQuote": true,
  "tabWidth": 2,
  "trailingComma": "es5",
  "useTabs": false,
  "importOrder": [
    "react",
    "^gatsby",
    "^react",
    "^((?!/).)*$",
    "<THIRD_PARTY_MODULES>",
    "^@atoms",
    "^@components",
    "^@molecules",
    "^@icons",
    "^[./]",
    "^[../]"
  ]
}
